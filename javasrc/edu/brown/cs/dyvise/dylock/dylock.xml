<DYLOCK>
   <TUPLEMODEL ABSTIME='true'>
      <TUPLE NAME='THREADLOCK'>
	 <FIELD NAME='THREAD' TYPE='THREAD' />
	 <FIELD NAME='START' TYPE='START_TIME' />
	 <FIELD NAME='END' TYPE='END_TIME' />
	 <FIELD NAME='STATE' TYPE='STRING' />
	 <FIELD NAME='LOCK' TYPE='OBJECT' />
      </TUPLE>
      <TUPLE NAME='THREADNOTIFY'>
	 <FIELD NAME='WHEN' TYPE='START_TIME' />
	 <FIELD NAME='THREAD' TYPE='THREAD' />
	 <FIELD NAME='TO' TYPE='THREAD' />
      </TUPLE>
      <TUPLE NAME='LOCKDATA'>
	 <FIELD NAME='START' TYPE='START_TIME' />
	 <FIELD NAME='END' TYPE='END_TIME' />
	 <FIELD NAME='LOCK' TYPE='OBJECT' />
	 <FIELD NAME='LEVEL' TYPE='INT' />
	 <FIELD NAME='TYPE' TYPE='INT' />
	 <FIELD NAME='TYPENAME' TYPE='STRING' />
	 <FIELD NAME='THREAD' TYPE='THREAD' />
      </TUPLE>
   </TUPLEMODEL>
   <GRAPHMODEL>
     <GRAPH NAME='Locks by thread' TYPE='TIMEROWS'>
	 <SELECT WHAT='ROWDATA'>
	    <TABLE DEFAULT='true' NAME='THREADLOCK' />
	 </SELECT>
	 <SELECT SIZE='10' WHAT='ROW'>
	    <FIELD DEFAULT='true' NAME='THREAD' SORT='SORT_NAME' TABLE='THREADLOCK' />
	 </SELECT>
	 <SELECT WHAT='HUE' >
	    <FIELD DEFAULT='true' NAME='LOCK' TABLE='THREADLOCK' />
	    <FIELD NAME='STATE' TABLE='THREADLOCK' />
	 </SELECT>
	 <SELECT WHAT='SAT'>
	    <CONST DEFAULT='true' NAME='NONE' NULL='0.0' VALUE='1.0' />
	    <CONST NAME='3/4' NULL='0' VALUE='0.75' />
	    <CONST NAME='2/4' NULL='0' VALUE='0.50' />
	    <CONST NAME='1/4' NULL='0' VALUE='0.25' />
	 </SELECT>
	 <SELECT WHAT='VALUE'>
	   <CONST DEFAULT='true' NAME='NONE' VALUE='1.0' />
	    <CONST NAME='3/4' NULL='0' VALUE='0.75' />
	    <CONST NAME='2/4' NULL='0' VALUE='0.50' />
	    <CONST NAME='1/4' NULL='0' VALUE='0.25' />
	 </SELECT>
	 <SELECT SIZE='5' WHAT='ROWITEM'>
	   <FIELD DEFAULT='true' NAME='STATE' TABLE='THREADLOCK' />
	   <FIELD NAME='LOCK' TABLE='THREADLOCK' />
	 </SELECT>
	 <SELECT WHAT='WIDTH'>
	   <CONST DEFAULT='true' NAME='NONE' VALUE='1.0' />
	 </SELECT>
	 <SELECT WHAT='ITEMWIDTH'>
	   <INTERVAL DEFAULT='true' TABLE='THREADLOCK' />
	 </SELECT>
	 <SELECT WHAT='LINKDATA'>
	   <TABLE DEFAULT='true' NAME='THREADNOTIFY' />
	 </SELECT>
	 <SELECT WHAT='DATA1'>
	   <FIELD DEFAULT='true' NAME='TO' TABLE='THREADNOTIFY' />
	 </SELECT>
	 <SELECT WHAT='DATA2'>
	   <FIELD DEFAULT='true' NAME='THREAD' TABLE='THREADNOTIFY' />
	 </SELECT>
	 <SELECT WHAT='LHUE'>
	   <CONST DEFAULT='true' NAME='NONE' VALUE='0' />
	 </SELECT>
	 <SELECT WHAT='LSAT'>
	   <CONST DEFAULT='true' NAME='COLORED' VALUE='1' />
	 </SELECT>
	 <SELECT WHAT='LVALUE'>
	   <CONST DEFAULT='true' NAME='COLORED' VALUE='1.0' />
	 </SELECT>
	 <SELECT WHAT='LENDHUE'>
	   <CONST DEFAULT='true' NAME='NONE' VALUE='0.333' />
	 </SELECT>
	 <SELECT WHAT='LENDSAT'>
	   <CONST DEFAULT='true' NAME='COLORED' VALUE='1' />
	 </SELECT>
	 <SELECT WHAT='LENDVALUE'>
	   <CONST DEFAULT='true' NAME='COLORED' VALUE='1.0' />
	 </SELECT>
	 <SELECT WHAT='LALPHA'>
	   <CONST DEFAULT='true' NAME='HALF' VALUE='0.5' />
	 </SELECT>
       </GRAPH>

      <GRAPH NAME='Data By Lock' TYPE='TIMEBLOCKS'>
	 <SELECT WHAT='ROWDATA'>
	    <TABLE DEFAULT='true' NAME='LOCKDATA' />
	 </SELECT>
	 <SELECT WHAT='ROW'>
	    <FIELD DEFAULT='true' NAME='LOCK' SORT='SORT_NAME' TABLE='LOCKDATA' />
	 </SELECT>
	 <SELECT WHAT='HUE' >
	    <FIELD DEFAULT='true' NAME='THREAD' TABLE='LOCKDATA' />
	    <FIELD NAME='TYPE' TABLE='LOCKDATA' />
	    <FIELD NAME='EVENT' TABLE='LOCKDATA' />
	 </SELECT>
	 <SELECT WHAT='SAT'>
	    <CONST DEFAULT='true' NAME='NONE' NULL='0.0' VALUE='1.0' />
	 </SELECT>
	 <SELECT WHAT='VALUE'>
	   <CONST DEFAULT='true' NAME='NONE' VALUE='1.0' />
	 </SELECT>
	 <SELECT WHAT='ALPHA'>
	   <INTERVAL DEFAULT='true' TABLE='THREADLOCK' />
	 </SELECT>
	 <SELECT WHAT='LEVEL'>
	   <FIELD DEFAULT='true' NAME='LEVEL' TABLE='LOCKDATA' />
	   <FIELD NAME='THREAD' TABLE='LOCKDATA' />
	 </SELECT>
	 <SELECT WHAT='FILL'>
	   <FIELD DEFAULT='true' NAME='TYPE' TABLE='LOCKDATA' />
	   <FIELD NAME='THREAD' TABLE='LOCKDATA' />
	   <FIELD NAME='EVENT' TABLE='LOCKDATA' />
	 </SELECT>
	 <SELECT WHAT='CENTER'>
	   <CONST DEFAULT='true' NAME='ZERO' NULL='0.0' VALUE='0.0' />
	 </SELECT>
       </GRAPH>

     </GRAPHMODEL>

</DYLOCK>
